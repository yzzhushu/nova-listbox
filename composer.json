{
    "name": "jshxl/list-box",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.10",
        "laravel/nova": "~4.0",
        "jshxl/primevue": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Jshxl\\ListBox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jshxl\\ListBox\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
