{
    "name": "jshxl/list-box",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^10.10",
        "laravel/nova": "~4.0",
        "jshxl/primevue": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Jshxl\\ListBox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jshxl\\ListBox\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
